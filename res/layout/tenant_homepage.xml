<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/egg_shell"
    tools:context=".TenantHomepage" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/textedit"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:padding="5dp"
        android:gravity="center_horizontal|center_vertical"
        	>
        	
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/black"
        	android:text="@string/welcome" />
        
        <LinearLayout
            android:layout_width="match_parent"
       		android:layout_height="wrap_content"
       		android:orientation="horizontal"
       		android:gravity="center_horizontal|center_vertical"
            >
        	<TextView
            	android:id="@+id/homepage_tenant_first_name_TextView"
        		android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		
            	android:textColor="@color/black"/>
        	<TextView
            	android:id="@+id/homepage_tenant_last_name_TextView"
        		android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:layout_marginLeft="5dp"
        		
            	android:textColor="@color/black"/>
        	
        </LinearLayout>
        
        <TextView
            android:id="@+id/homepage_tenant_email_TextView"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
            android:textColor="@color/black" />
        
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal|center_vertical"
        	>
        	
        <Button
            android:id="@+id/homepage_tenant_logout_button"
            android:background="@drawable/button"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_marginRight="5dp"
            android:gravity="center_horizontal|center_vertical"
			android:layout_weight="1"
            android:text="@string/logout" />
        
        <Button
            android:id="@+id/homepage_tenant_edit_button"
            android:background="@drawable/button"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_marginLeft="5dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:gravity="center_horizontal|center_vertical"
			android:layout_weight="1"
            android:text="@string/edit_info" />
        
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:orientation="vertical"
        android:layout_weight="1"
        android:background="@drawable/table"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:padding="10dp"
        android:gravity="center_horizontal|center_vertical"
        	>
        
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display1"
        	    	android:text="@string/landlord_name"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_landlord_first_name_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        		
        		<TextView
        	    	android:id="@+id/homepage_tentant_landlord_last_name_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="3dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
		</LinearLayout>
		
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display9"
        	    	android:text="@string/landlord_phone_number"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_landlord_phone_number_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
		</LinearLayout>
		
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display2"
        	    	android:text="@string/landlord_email"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_landlord_email_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
		</LinearLayout>
		
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display3"
        	    	android:text="@string/property_name"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_property_name_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
		</LinearLayout>
        
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display4"
        	    	android:text="@string/unit_number"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_unit_number_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
			</LinearLayout>
			
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display5"
        	    	android:text="@string/monthly_rent"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        		
        		<TextView
        		    android:id="@+id/homepage_tentant_display6"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:text="@string/dollar"
            		android:layout_marginLeft="3dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_monthly_rent_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="2dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
			</LinearLayout>
			
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display7"
        	    	android:text="@string/lease_start_date"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_start_date_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
			</LinearLayout>
			
        <LinearLayout
    	        android:layout_marginTop="5dp"
    	        android:gravity="center_horizontal|center_vertical"
    	        android:layout_width="match_parent"
        		android:layout_height="wrap_content"
    	        android:orientation="horizontal">

        		<TextView
        		    android:id="@+id/homepage_tentant_display8"
        	    	android:text="@string/lease_end_date"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		
            		android:gravity="center_horizontal"
            		android:textStyle="bold"
            		android:textColor="@color/black" />
        	
        		<TextView
        	    	android:id="@+id/homepage_tentant_end_date_TextView"
            		android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:layout_marginLeft="5dp"
            		
            		android:gravity="center_horizontal"
            		android:textColor="@color/black" />
        	
			</LinearLayout>
        
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:gravity="center_horizontal|center_vertical"
        	>
        	
        <Button
            android:id="@+id/homepage_tenant_request_button"
            android:background="@drawable/button"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
			android:layout_weight="1"
            android:text="@string/make_request" />
        
    </LinearLayout>
    
    
    

</LinearLayout>